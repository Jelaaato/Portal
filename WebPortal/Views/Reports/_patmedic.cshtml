@*@model WebPortal.Models.OPSViewModel*@
@model PagedList.IPagedList<WebPortal.Models.webportal_patient_medication>
@using PagedList.Mvc;
@using PagedList;
<div id="tbl-wrapper3">
    <ul class="list-group row">
                <li class="list-group-item col-xs-6"><h5>Date</h5></li>
                <li class="list-group-item col-xs-6"><h5>Details</h5></li>

                @foreach (var item in Model)
                {
                    <li class="list-group-item col-xs-6">
                        <h6>@Html.DisplayFor(model => item.note_date)</h6>
                    </li>
                    <li class="list-group-item col-xs-6">
                        <h6>@Html.DisplayFor(model => item.details)</h6>
                    </li>
                }
            </ul>
    Page @(Model.PageCount < Model.PageNumber ? 0 : Model.PageNumber) of @Model.PageCount
    @Html.PagedListPager(Model, page => Url.Action("OMCPpatientMedication", new { page, currentfilter = ViewBag.CurrentFilter }), PagedListRenderOptions.EnableUnobtrusiveAjaxReplacing(
        new AjaxOptions()
        {
            HttpMethod = "GET",
            UpdateTargetId = "tbl-wrapper3"
        }))
        </div>

      
